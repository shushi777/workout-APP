{
  "name": "Workout Video Editor",
  "short_name": "WorkoutVid",
  "description": "Split workout videos and build exercise library",
  "start_url": "/",
  "display": "standalone",
  "theme_color": "#667eea",
  "background_color": "#ffffff",
  "orientation": "portrait-primary",
  "icons": [],
  "share_target": {
    "action": "/share-receiver",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "video",
          "accept": ["video/mp4", "video/avi", "video/mov", "video/mkv", "video/flv", "video/wmv", "video/*"]
        }
      ]
    }
  },
  "categories": ["health", "fitness", "lifestyle"],
  "screenshots": []
}
